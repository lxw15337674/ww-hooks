{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "wwhooks": [
        "src"
      ],
      "wwhooks/*": [
        "src/*",
        "*"
      ]
    }
  },
  "include": [
    ".dumi/**/*",
    ".dumirc.ts",
  ],
  "exclude": [
    "node_modules",
    "lib",
    "es",
    "dist",
    "typings",
    "**/__test__",
    "test",
    "docs",
    "tests"
  ]
}